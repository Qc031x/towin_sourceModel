<script>
    var setting={
        callback : {
            onClick : setFrame
        }
    };
    var zNodes = [
        {name : "华润深圳", open : true, children : [
            {name : "总经办", open : true},
            {name : "技术部", children : [
                {name : "项目组1"},
                {name : "项目组2"},
                {name : "项目组3"},
                {name : "项目组4"}
            ]},
            {name : "财务部"},
            {name : "售后部", children : [
                {name : "售后一部"},
                {name : "售后二部"},
                {name : "售后三部"}
            ]},
            {name : "后勤部", children : [
                {name : "后勤一部"},
                {name : "后勤二部"},
                {name : "后勤三部"}
            ]},
            {name : "法务部"}
        ]}
    ];
    $(document).ready(function () {
        //数据超过一页显示分页
        if ($('tbody tr').size() > 8) {
            $(".tcdPageCode").css('display', 'block');
        }
        else {
            $(".tcdPageCode").css('display', 'none');
        }

        $.fn.zTree.init($("#treeDemo"), setting, zNodes);
        var obj = $.fn.zTree.getZTreeObj("treeDemo");
        obj.expandAll(true);
    });

    function setFrame(event, treeId, treeNode) {
        if(!treeNode.isParent){
            $("#liebiao1").show();
            if(Number(treeNode.getIndex()) % 2 == 0){
                $("#liebiao1 tr:gt(5)").hide();
            }else{
                $("#liebiao1 tr").show();
            }
        }
    };
</script>
